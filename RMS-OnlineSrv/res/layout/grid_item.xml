<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/LinearLayout01" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:orientation="horizontal"
	android:layout_gravity="center_vertical">
	<TextView android:id="@+id/item1" android:layout_height="fill_parent"
		android:layout_width="wrap_content" android:width="20dip"></TextView>
	<TextView android:id="@+id/orderItem" android:layout_height="wrap_content"
		android:textSize="14dip" android:width="50dip" android:layout_width="wrap_content"></TextView>
	<TextView android:id="@+id/nameItem" android:layout_height="wrap_content"
		android:textSize="14dip" android:width="50dip" android:layout_width="wrap_content"></TextView>
	<TextView android:id="@+id/countItem" android:textSize="14dip"
		android:width="40dip" android:layout_width="wrap_content"
		android:layout_height="fill_parent"></TextView>
	<TextView android:id="@+id/amountItem" android:layout_height="wrap_content"
		android:width="50dip" android:textSize="14dip" android:layout_width="wrap_content"></TextView>
	<TextView android:id="@+id/formatItem" android:layout_height="wrap_content"
		android:width="30dip" android:textSize="14dip" android:layout_width="wrap_content"></TextView>
	<TextView android:id="@+id/statusItem" android:layout_height="wrap_content"
		android:width="50dip" android:textSize="14dip" android:layout_width="wrap_content"></TextView>
	<TextView android:id="@+id/dateItem" android:layout_height="wrap_content"
		android:width="100dip" android:textSize="14dip" android:layout_width="wrap_content"></TextView>
	<CheckBox android:id="@+id/actionItem" android:layout_height="wrap_content"
		android:width="100dip" android:layout_width="wrap_content"
		android:checked="false"></CheckBox>
</LinearLayout>